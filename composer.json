{
    "name": "fusible/view-provider",
    "description": "Aura\\View Provider for Aura\\Di",
    "homepage":"https://github.com/fusible/fusible.view-provider",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
         "aura/di": "~4.0",
         "aura/view": "^2.3",
         "aura/html": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Fusible\\ViewProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fusible\\ViewProvider\\":"tests/",
            "Aura\\Di\\": "vendor/aura/di/tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    }
}
